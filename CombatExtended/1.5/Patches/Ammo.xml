<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!-- Just renames 303 british -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingCategoryDef[defName="Ammo303British"]/label</xpath>
		<value>
			<label>.30-06 Springfield</label>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/CombatExtended.AmmoSetDef[defName="AmmoSet_303British"]/label</xpath>
		<value>
			<label>.30-06 Springfield</label>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[@Name="303BritishBase"]/description</xpath>
		<value>
			<description>An outdated rifle cartridge designed by Springfield Armory in 1906 AD.</description>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Ammo_303British_FMJ"]/label</xpath>
		<value>
			<label>.30-06 Springfield cartridge (FMJ)</label>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Ammo_303British_AP"]/label</xpath>
		<value>
			<label>.30-06 Springfield cartridge (AP)</label>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Ammo_303British_HP"]/label</xpath>
		<value>
			<label>.30-06 Springfield cartridge (HP)</label>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Bullet_303British_FMJ"]/label</xpath>
		<value>
			<label>.30-06 Springfield bullet (FMJ)</label>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Bullet_303British_AP"]/label</xpath>
		<value>
			<label>.30-06 Springfield bullet (AP)</label>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Bullet_303British_HP"]/label</xpath>
		<value>
			<label>.30-06 Springfield bullet (HP)</label>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_FMJ"]/label</xpath>
		<value>
			<label>make .30-06 Springfield (FMJ) cartridge x80</label>
		</value>
	</Operation>
		<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_AP"]/label</xpath>
		<value>
			<label>make .30-06 Springfield (AP) cartridge x80</label>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_HP"]/label</xpath>
		<value>
			<label>make .30-06 Springfield (HP) cartridge x80</label>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_FMJ"]/description</xpath>
		<value>
			<description>Craft 80 .30-06 Springfield (FMJ) cartridges.</description>
		</value>
	</Operation>
		<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_AP"]/description</xpath>
		<value>
			<description>Craft 80 .30-06 Springfield (AP) cartridges.</description>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_HP"]/description</xpath>
		<value>
			<description>Craft 80 .30-06 Springfield (HP) cartridges.</description>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_FMJ"]/jobString</xpath>
		<value>
			<jobString>Making .30-06 Springdield (FMJ) cartridges.</jobString>
		</value>
	</Operation>
		<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_AP"]/jobString</xpath>
		<value>
			<jobString>Making .30-06 Springdield (AP) cartridges.</jobString>
		</value>
	</Operation>
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_HP"]/jobString</xpath>
		<value>
			<jobString>Making .30-06 Springdield (HP) cartridges.</jobString>
		</value>
	</Operation>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Parabellum</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
	
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="Ammo_303British_I"]/label</xpath>
					<value>
						<label>.30-06 Springfield cartridge (I)</label>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="Bullet_303British_I"]/label</xpath>
					<value>
						<label>.30-06 Springfield bullet (i)</label>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_I"]/label</xpath>
					<value>
						<label>make .30-06 Springfield (I) cartridge x80</label>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_I"]/description</xpath>
					<value>
						<description>Craft 80 .30-06 Springfield (I) cartridges.</description>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName="MakeAmmo_303British_I"]/jobString</xpath>
					<value>
						<jobString>Making .30-06 Springdield (I) cartridges.</jobString>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>


</Patch>